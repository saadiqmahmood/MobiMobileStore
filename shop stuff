        <!-- CODE FROM https://www.codehim.com/collections/javascript-shopping-cart-examples-with-demo/ BUT MODIFIED -->
        <!-- CART TABLE -->
        <div class="col-sm-3">
          <table class="cart-table">
            <tr>
              <!-- CART PRODUCT AND PRICE -->
              <th>Product</th>
              <th>Price</th>
            </tr>
            <tbody id="carttable"></tbody>
          </table>
          <hr />
          <table class="cart-table" id="carttotals">
            <tr>
              <!-- CART ITEMS AND TOTAL -->
              <td>Items</td>
              <td>Total</td>
            </tr>
            <tr>
              <!-- ITEMS QUANTITY AND TOTAL SET TO 0 -->
              <td>x <span id="itemsquantity">0</span></td>

              <td>$<span id="total">0</span></td>
            </tr>
          </table>
          <!-- EMPTY CART AND CHECKOUT DIV -->
          <div class="cart-buttons">
            <!-- CREATED BUTTONS FOR EMPTY CART AND CHECKOUT -->
            <button class="cartbutton" id="emptycart">Empty Cart</button>
            <button class="cartbutton" id="checkout" onClick="checkoutNow()">Checkout</button>
          </div>
        </div>
        <!-- MY CODE START -->
        <div id="checkOut" class="col-sm-3" style="display: none;">
            <h1 
                style="
                font-size: 30px;
                font-family: 'Trebuchet MS', 'Lucida Sans Unicode', 'Lucida Grande', 'Lucida Sans', Arial, sans-serif;
                text-align: center;
                "
            >
                CHECKOUT
            </h1>
            <form>
            <ul>
              <li>
                <!-- FOR NAME INPUT -->
                <input class="input2" type="text" name="name" placeholder="Name" />
              </li>
              <li>
                <!-- FOR EMAIL INPUT -->
                <input class="input2" type="email" name="email" placeholder="Email"/>
                <br /><br />
              </li>
              <li>
                <!-- FOR ADDRESS INPUT -->
                <textarea class="textarea2" name="address" placeholder="Address"></textarea>
              </li>
              <li>
                <!-- SUBMIT BUTTON -->
                <input type="submit" value="Checkout" />
              </li>
            </ul>
          </form>
        </div>
      </div>